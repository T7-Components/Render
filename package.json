{
  "name": "@t7-components/render",
  "version": "0.0.0",
  "description": "Conditionally render children when a condition is met",
  "author": "TandemSeven <github@tandemseven.com>",
  "license": "MIT",
  "repository": "https://github.com/t7-components/render",
  "main": "build/index.js",
  "jest": {
    "collectCoverageFrom": [
      "**/source/*.js"
    ]
  },
  "scripts": {
    "build": "npm run test && rimraf build/* && webpack",
